<?xml version="1.0"?>
<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
    <sitecore>
        <unicorn>
            <configurations>
                <configuration name="Feature.UserInterface" description="User Interface for the URL Shortening Service" extends="Helix.Feature" dependencies="Foundation.*">
                    <predicate type="Unicorn.Predicates.SerializationPresetPredicate, Unicorn" singleInstance="true">
                        <include name="Feature.UserInterface.Chunks" database="core" path="/sitecore/content/Applications/Content Editor/Ribbons/Chunks/Short URL" />
                        <include name="Feature.UserInterface.Strips" database="core" path="/sitecore/content/Applications/Content Editor/Ribbons/Strips/Home/Short URL" />
                        <!--<include name="UserInterface.Translations" database="core" path="/sitecore/system/Dictionary/Modules/Short URL" />-->
                    </predicate>
                </configuration>
            </configurations>
        </unicorn>
    </sitecore>
</configuration>